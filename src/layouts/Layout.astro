---

// component imports
import MainHead from './MainHead.astro';
import Nav from '../components/Nav.astro';
import Footer from '../components/Footer.astro';

// style imports
import "../styles/global.css";

// props
const { 
  title = "My Astro Blog", 
  description  = "My musings about the Astro framework" ,
  image,
  frontmatter,
  robots,
} = Astro.props;
---

<!doctype html>
<html lang="en">
  <MainHead {title} {description} {image} {frontmatter} {robots} />
  <body>
	  <Nav />
    <main>
      <slot />
    </main>
	  <Footer />
  </body>
</html>
